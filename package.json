{
  "name": "email-assistant",
  "version": "0.0.1",
  "private": true,
  "description": "Intelligent Email Assistant powered by Mastra AI",
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
